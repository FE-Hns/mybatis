<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--
    配置我们的映射实体类
    通过命名空间找到我们的实体类
-->
<mapper namespace="com.neisun.entity.Users">
    <select id="findAll"
            resultType="com.neisun.entity.Users">
        select * from users
    </select>
</mapper>